SELECT 
  pheno_entity, hpo.pheno_name, count(pheno_entity), (array_agg(array_to_string(string_to_array(si.words, '|^|'), ' ')))[1] 
FROM genepheno_causation_is_correct_inference gc
  JOIN
  hpo_abnormalities hpo
    ON (hpo.hpo_id = gc.pheno_entity)
    join sentences_input si on (gc.doc_id = si.doc_id and gc.section_id = si.section_id and gc.sent_id = si.sent_id)
WHERE 
  expectation > 0.9
GROUP BY pheno_entity, hpo.pheno_name
ORDER BY count ASC
LIMIT 1000
