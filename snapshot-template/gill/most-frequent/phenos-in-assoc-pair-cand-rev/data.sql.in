SELECT 
  pheno_entity, hpo.pheno_name, count(pheno_entity), (array_agg(array_to_string(string_to_array(si.words, '|^|'), ' ')))[1]
FROM genepheno_association ga
  JOIN
  hpo_abnormalities hpo
    ON (ga.pheno_entity = hpo.hpo_id)
    join sentences_input si on (ga.doc_id = si.doc_id and ga.section_id = si.section_id and ga.sent_id = si.sent_id)
WHERE 
  is_correct != 'f' OR is_correct IS NULL 
GROUP BY pheno_entity, hpo.pheno_name
ORDER BY count ASC
LIMIT 1000
